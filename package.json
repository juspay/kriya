    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "npm run build:watch"
  },
  "keywords": [
    "automation",
    "web-automation",
    "kriya",
    "form-filling",
    "dom-manipulation",
    "ai-actions",
